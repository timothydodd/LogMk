{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "ConnectionStrings": {
        "MyDatabase": "Server=your_server;Database=your_db;User Id=your_user;Password=your_password;"
    },
    "ServiceConfig": {
        "Endpoint": "https://api.example.com",
        "ApiKey": "your_api_key"
    },
    "LogWatcherOptions": {
        "Paths": [
            "/var/log/containers",
            "/var/log/applications",
            "/var/log/system"
        ]
    }
}
