{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "System.Net.Http.HttpClient": "Warning"
        }
    },
    "LoggingApi": {
        "BaseUrl": "http://192.168.2.49:5278"
    },
    "LogWatcherOptions": {
        "LogLevel": {
            "Default": "Information",
            "speaker": "Information",
            "logmk-agent-container": "Warning"
        },
        "IgnorePods": [
            "dashboard-metrics-scraper"
        ],
        "Paths": [
            "/var/log/pods"
        ],
        "FilePatterns": [
            "*.log",
            "*.txt"
        ],
        "MaxDaysOld": 30
    },
    "BatchingOptions": {
        "MaxBatchesPerMinute": 60,
        "MaxQueueSize": 10000,
        "MaxRetryQueueSize": 5000
    }
}
