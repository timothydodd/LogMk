{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "System.Net.Http.HttpClient": "Warning"
        }
    },
    "LoggingApi": {
        "BaseUrl": "http://logmk-api-lb"
    },
    "LogWatcherOptions": {
        "LogLevel": {
            "Default": "Information",
            "speaker": "Error",
            "logmk-agent-container": "Warning"
        },
        "IgnorePods": [
            "dashboard-metrics-scraper",
            "coredns"
        ],
        "Paths": [
            "/var/log/pods"
        ]
    }
}
