{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "CorsOrigins": "",
    "ConnectionStrings": {
        "DefaultConnection": "Server=.;Database=IdentityServer4;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "AllowedHosts": "*",
    "JwtSettings": {
        "Secret": "your_secret_key_here",
        "Issuer": "logmk",
        "Audience": "logmk",
        "ExpiryMinutes": 60,
        "RefreshTokenExpiryDays": 30
    },
    "LogSettings": {
        "MaxDaysOld": 30,
        "EnableDuplicateDetection": true,
        "FingerprintDeduplicationWindowSeconds": 30,
        "SignalRBroadcastLimit": 500,
        "PodCacheTtlMinutes": 60,
        "PurgeBatchSize": 10000
    }
}
    
